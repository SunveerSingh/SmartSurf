<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.devzeta.smartsurf">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".youtubemain.youtubeframents.techincal"></activity>
        <activity android:name=".music.musicfragments.spotify" />
        <activity android:name=".youtubemain.youtubemainscreen" />
        <activity android:name=".web.webmainscreen" />
        <activity android:name=".shopping.shoppingmainscreen" />
        <activity android:name=".news.newsmainscreen" />
        <activity android:name=".music.musicmainscreen" />
        <activity android:name=".dictionary.dictionarymainscreen" />
        <activity android:name=".youtube" />
        <activity android:name=".disctionarysearch" />
        <activity android:name=".newssearch" />
        <activity android:name=".musicsearch" />
        <activity android:name=".shoppingsearch" />
        <activity android:name=".websearch" />
        <activity android:name=".homescreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>